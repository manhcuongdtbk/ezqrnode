{
  "extends": ["airbnb-base", "prettier", "plugin:node/recommended"],
  "parser": "babel-eslint",
  "plugins": ["prettier", "babel"],
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "warn",
    "no-console": "off",
    "func-names": "off",
    "no-process-exit": "off",
    "object-shorthand": "off",
    "class-methods-use-this": "off"
  }
}
